# 编译psg1218a固件:
CONFIG_TARGET_ramips=y 
CONFIG_TARGET_ramips_mt7620=y
CONFIG_TARGET_ramips_mt7620_DEVICE_phicomm_psg1218a=y

# 固件压缩:
CONFIG_TARGET_IMAGES_GZIP=y

# IPv6支持:
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_ip6tables=y
CONFIG_PACKAGE_ipv6helper=y

#去除插件
# CONFIG_PACKAGE_luci-app-unblockmusic is not set
# CONFIG_PACKAGE_luci-app-ddns is net set
# CONFIG_PACKAGE_luci-app-accesscontrol is net set
# CONFIG_PACKAGE_luci-app-vsftpd is net set
# CONFIG_PACKAGE_luci-app-autoreboot is net set
# CONFIG_PACKAGE_luci-app-filetransfer is net set
# CONFIG_PACKAGE_luci-app-nlbwmon is net set 
# CONFIG_PACKAGE_luci-app-ssr-plus is net set
# CONFIG_PACKAGE_luci-app-turboacc is net set
# CONFIG_PACKAGE_luci-newapi is net set


# 常用LuCI插件选择:
CONFIG_DEFAULT_luci-app-arpbind=y
CONFIG_DEFAULT_luci-app-mtwifi=y
CONFIG_DEFAULT_luci-app-upnp=y
CONFIG_DEFAULT_luci-app-ramfree=y
CONFIG_DEFAULT_luci-app-flowoffload=y
CONFIG_DEFAULT_luci-app-vlmcsd=y

# LuCI主题:
# CONFIG_PACKAGE_luci-theme-opentomcat is not set
CONFIG_PACKAGE_luci-theme-netgear=y
# CONFIG_PACKAGE_luci-theme-argon is not set
# CONFIG_DEFAULT_luci-theme-bootstrap-mod is not set
# CONFIG_DEFAULT_luci-theme-darkmatter is not set
# CONFIG_PACKAGE_luci-theme-material is not set